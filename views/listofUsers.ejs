<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body class="container" >
    <section class="userslist">
        <h2>My Flat Mate</h2>
        <% if(users.length){ %>
            <% users.forEach(user => { %>
                <a href="/users/<%= user.email %>">
                    <h3 class="userlist"><%= user.name %></h3>
                </a>
            <% }) %>
        <%} else { %>
            <h3>No Flat Mate available</h3>
        <%}%>
        <a href="/users/new" class="addmore">Add</a>
    </section>
</body>
</html>